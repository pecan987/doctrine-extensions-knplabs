{
  "name": "nettrine/extensions-knplabs",
  "description": "Doctrine2 behavioral extensions for Nette Framework",
  "keywords": ["contributte", "nette", "doctrine", "orm", "database"],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/doctrine-extensions-knplabs",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.2",
    "knplabs/doctrine-behaviors": "^2",
    "nette/security": "*",
    "contributte/di": "dev-master",
    "tracy/tracy": "*"
  },
  "require-dev": {
    "phpstan/extension-installer": "*",
    "phpstan/phpstan-deprecation-rules": "*",
    "phpstan/phpstan-nette": "*",
    "phpstan/phpstan-strict-rules": "*"
  },
  "autoload": {
    "psr-4": {
      "Nettrine\\Extensions\\KnpLabs\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Cases\\": "tests/cases",
      "Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.3.x-dev"
    }
  }
}
